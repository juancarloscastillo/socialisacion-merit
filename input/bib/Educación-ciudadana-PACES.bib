@article{kwon_Localized_2020,
  title = {Localized Social Media and Civic Life: {{Motivations}}, Trust, and Civic Participation in Local Community Contexts},
  shorttitle = {Localized Social Media and Civic Life},
  author = {Kwon, K. Hazel and Shao, Chun and Nah, Seungahn},
  year = {2020},
  publisher = {{Routledge Journals, Taylor \& Francis Ltd}},
  address = {{Abingdon}},
  issn = {1933-1681},
  doi = {10.1080/19331681.2020.1805086},
  abstract = {This study investigates the ways in which the localized use of social media influences civic life in local communities. Specifically, the study explores the direct and indirect effects of informational and social motivations underlying localized social media uses on trust-building (interpersonal trust, political trust, and trust in community organizations) and civic participation in online and offline contexts. The findings through a nationwide survey of U.S. adults indicate that localized social media use resulted in the increase of interpersonal trust. Furthermore, the results show that trust in community organizations and local political trust played a more important role as a mediator between localized social media use and civic participation. Notably, localized social media use driven by social motivations had a greater effect on trust and civic participation than informational motivation. Theoretical and practical implications are discussed for civic technology and community building.},
  journal = {Journal of Information Technology \& Politics},
  keywords = {citizens,civic participation,communication,engagement,internet,interpersonal trust,local political trust,Localized social media,news media,online,opinion,political trust,social motivations,students,trust in community organizations,videomalaise},
  language = {English}
}

