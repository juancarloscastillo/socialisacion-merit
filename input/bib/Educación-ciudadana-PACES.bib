@incollection{villalobos_School_2018,
  title = {School {{Segregation}} of {{Immigrant Students}}},
  booktitle = {Teaching {{Tolerance}} in a {{Globalized World}}},
  author = {Villalobos, Crist{\'o}bal and Trevi{\~n}o, Ernesto and Wyman, Ignacio and B{\'e}jares, Consuelo},
  editor = {{Sandoval-Hern{\'a}ndez}, Andr{\'e}s and Isac, Maria Magdalena and Miranda, Daniel},
  year = {2018},
  volume = {4},
  pages = {67--86},
  publisher = {{Springer International Publishing}},
  address = {{Cham}},
  doi = {10.1007/978-3-319-78692-6_5},
  abstract = {El Estudio Internacional de Educaci\'on C\'ivica y Ciudadana (ICCS) de la IEA identifica a los inmigrantes de primera y segunda generaci\'on y, por lo tanto, puede revelar patrones de segregaci\'on de estudiantes inmigrantes. Desde una perspectiva comparativa, estos pueden ser analizados para proporcionar la distribuci\'on, concentraci\'on y propagaci\'on de estudiantes inmigrantes entre escuelas y pa\'ises. Aqu\'i tres m\'etodos de an\'alisis fueron empleados: (1) an\'alisis descriptivo, (2) construcci\'on de \'indices de segregaci\'on, y (3) an\'alisis multinivel. En general, los pa\'ises no implementan pol\'iticas sistem\'aticas para concentrar o segregar a los estudiantes inmigrantes, aunque existen importantes diferencias entre pa\'ises; en cambio, es necesario apreciar una fuerte relaci\'on entre niveles de segregaci\'on y \'indices de desigualdad o desarrollo humano y considerar factores geogr\'aficos, culturales y econ\'omicos. Las escuelas parecen tener un efecto limitado en la transformaci\'on de las actitudes hacia la inmigraci\'on.},
  isbn = {978-3-319-78691-9 978-3-319-78692-6},
  keywords = {\#duplicate-citation-key},
  language = {English}
}

