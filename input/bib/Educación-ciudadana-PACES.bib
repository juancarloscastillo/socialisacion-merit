@incollection{hartung_Selective_2017,
  title = {Selective {{Hearing}}: {{Rethinking Children}} and {{Young People}}'s `{{Voice}}'},
  shorttitle = {Selective {{Hearing}}},
  booktitle = {Conditional {{Citizens}}: {{Rethinking Children}} and {{Young People}}'s {{Participation}}},
  author = {Hartung, Catherine},
  editor = {Hartung, Catherine},
  year = {2017},
  pages = {77--104},
  publisher = {{Springer}},
  address = {{Singapore}},
  doi = {10.1007/978-981-10-3938-6_5},
  abstract = {This chapter examines how the notion of children and young people's `voice' is reiterated in the field of children and young people's participation. The chapter argues that the word is often used to emphasise children and young people's voice as pure , fixed and conveniently in line with what is `right' for society. In examining these assumptions, the chapter draws on five examples of situations where children and young people's voices did not comply with these assumptions or were more ambiguous. More specifically, the chapter draws on examples of children and young people's voices `we don't want to hear' in Australia, children and young people's voices in legal proceedings in Europe, children and young people with special communication needs in England, children and young people's silences in New Zealand, and children and young people's voice, and resistances to voice, in schools in the United States.},
  isbn = {978-981-10-3938-6},
  keywords = {Authorial Voice,Lansdown,Student Outreach,Young People,Yousafzai},
  language = {English},
  series = {Perspectives on {{Children}} and {{Young People}}}
}

