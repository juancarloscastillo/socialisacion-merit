@article{kraaykamp_Parental_2016,
  title = {Parental Cultural Socialization and Educational Attainment. {{Trend}} Effects of Traditional Cultural Capital and Media Involvement},
  author = {Kraaykamp, Gerbert and Notten, Natascha},
  year = {2016},
  month = sep,
  volume = {45},
  pages = {63--71},
  publisher = {{Elsevier Sci Ltd}},
  address = {{Oxford}},
  issn = {0276-5624},
  doi = {10.1016/j.rssm.2016.08.003},
  abstract = {This article analyzes long-term developments in parental cultural socialization effects for children's educational attainment. Retrospective information of 3.106 respondents from the Family Survey of the Dutch population are used to address questions on trends in the impact of traditional measures of parental cultural capital and the impact of parental media involvement activities. Foremost, our study highlights that the relevance of traditional parental cultural capital for children's educational success in the Netherlands diminishes over time, while parental media involvement activities became more important. Over the past decades, especially parental reading instruction and parents' setting television restriction rules became increasingly more meaningful for children's educational performance. (C) 2016 Elsevier Ltd. All rights reserved.},
  journal = {Research in Social Stratification and Mobility},
  keywords = {academic-performance,achievement,childhood,Educational attainment,home literacy,intergenerational transmission,literary education,Parental cultural capital,Parental media involvement,participation,Retrospective information,social-inequality,television,Trends,united-states},
  language = {English}
}

