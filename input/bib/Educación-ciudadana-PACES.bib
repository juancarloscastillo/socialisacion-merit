@article{halvorsen_sense_2020,
  title = {A Sense of Unease: Elite High School Students Negotiating Historical Privilege},
  shorttitle = {A Sense of Unease},
  author = {Halvorsen, P{\aa}l},
  year = {2020},
  month = nov,
  volume = {0},
  pages = {1--16},
  publisher = {{Routledge}},
  issn = {1367-6261},
  doi = {10.1080/13676261.2020.1844169},
  abstract = {The Scandinavian public school systems are free of charge and are central institutions upholding the egalitarian ideals that these societies are famous for. However, they do provide some students with privileged resources and enables trajectories that are out of reach for others. In this article, we address how students at two elite high schools in central Oslo acquire knowledge and relate to the `eliteness' of their schools. Through individual interviews with 73 students we arrive at a closer understanding of elite schools and privileged students in the Norwegian school system. The two schools, Oslo Commerce School (OCS) and Schola Osloensis (SO), share elite characteristics, but have strikingly different school cultures. Their elite identification is examined through looking at their attitude towards learning, school work and elite status. The study unfolds how the `ease' often associated with elite education is not necessarily developed, and is acquired in different ways. This glimpse into Norwegian elite schooling offers a challenge to the generalizability of studies from more market-oriented school systems.},
  annotation = {\_eprint: https://doi.org/10.1080/13676261.2020.1844169},
  journal = {Journal of Youth Studies},
  keywords = {Egalitarianism,elite education,privilege,schoolwork,youth},
  number = {0}
}

@article{han_Who_2016,
  title = {Who Supports Democracy? {{Evidence}} from a Survey of {{Chinese}} Students and Scholars in the {{United States}}},
  shorttitle = {Who Supports Democracy?},
  author = {Han, Donglin and Chen, Dingding},
  year = {2016},
  month = jun,
  volume = {23},
  pages = {747--769},
  publisher = {{Routledge Journals, Taylor \& Francis Ltd}},
  address = {{Abingdon}},
  issn = {1351-0347},
  doi = {10.1080/13510347.2015.1017566},
  abstract = {Does the experience of living in the United States lead Chinese people to believe that China should pursue democracy? Our study produced mixed results, as we found that overseas study resulted in significant attitude changes by various means. Education in social sciences and consumption of foreign media were both associated with supportive attitudes toward democracy, and also led respondents to believe that Western liberal democracy is a good thing and to push for it. Conversely, living overseas had a negative effect on respondents' support for Chinese democratization, as the longer they had lived in the United States, the better they understood Western democracy and were suspicious about whether China should pursue immediate democratization. Meanwhile, we also find Chinese media has a negative impact on public support for democracy, both instrumental and intrinsic. Finally, factors associated with attitudes toward democracy, such as traditional values, self-expression values, and perceived performance of the Chinese government, were also significant.},
  journal = {Democratization},
  keywords = {abroad,africa,attitudes,China,foreign,government,institutions,legitimacy,overseas students,performance,support for democracy,survey,transition,us},
  language = {English},
  number = {4}
}

