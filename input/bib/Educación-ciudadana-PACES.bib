@article{irwin_Family_2009b,
  title = {Family Contexts, Norms and Young People's Orientations: Researching Diversity},
  shorttitle = {Family Contexts, Norms and Young People's Orientations},
  author = {Irwin, Sarah},
  year = {2009},
  month = aug,
  volume = {12},
  pages = {337--354},
  publisher = {{Routledge}},
  issn = {1367-6261},
  doi = {10.1080/13676260902807235},
  abstract = {The paper presents an analysis of young people's orientations, and expectations for the future, as they relate to family context and socio-economic background. It draws on data from both the Longitudinal Study of Young People in England and from `Young Lives and Times', part of the ESRC National Centre for Research Methods. The latter study has generated both quantitative and qualitative data, and these are brought together directly in developing the arguments in the paper. The paper explores young people's perceptions of family life, and offers new evidence and analysis of the value they place on emotional support. Attitudinal data show that such support is seen as very important to future success alongside more individualistic, meritocratic, criteria. The paper then explores family contexts and youngsters' orientations to schoolwork, and their expectations for their future education. In respect of the latter, socio-economic background becomes key as a predictor of difference. Through strategically linking qualitative and quantitative data, the paper develops an analysis of individuals' orientations and expectations as these relate to proximate circumstances. Linking quantitative and qualitative evidence in this way helps provide insights into diverse subjectivities, and how they articulate with diverse, class-varying, normal expectations.},
  annotation = {\_eprint: https://doi.org/10.1080/13676260902807235},
  journal = {Journal of Youth Studies},
  keywords = {attitudes,emotional support,expectations,family,meritocratic socialization paper,qualitative,quantitative,young people},
  number = {4}
}

@article{isac_Native_2012,
  title = {{Native Students Attitudes towards Equal Rights for Immigrants. A Study in 18 European Countries}},
  author = {Isac, Maria Magdalena and Maslowski, Ralf and {van der Werf}, Greetje},
  year = {2012},
  month = apr,
  issn = {1618-5293},
  doi = {10.4119/jsse-586},
  abstract = {El presente estudio investiga los determinantes de las actitudes de los estudiantes nativos hacia la igualdad de derechos de los inmigrantes, prestando especial atenci\'on al efecto de la participaci\'on de los inmigrantes en el aula y la medida en que puede ser generalizada en los contextos de los pa\'ises. La contribuci\'on arroja una nueva luz sobre la validez de la hip\'otesis del contacto, que sugiere que la mezcla de los nativos y los estudiantes inmigrantes en las escuelas y aulas pueden contribuir a mayores niveles de apoyo a los derechos de los inmigrantes. Los an\'alisis se realizaron en los 18 pa\'ises que participaron en el estudio de ICCS en 2009. En los an\'alisis aplicamos un modelo multinivel de tres niveles controlando para el individuo, caracter\'isticas del aula, y del pa\'is. Probamos una pendiente aleatoria para de los inmigrantes en el aula a nivel nacional, y modelamos ambos efectos lineales y cuadr\'aticos de la cuota de los inmigrantes. El patr\'on general sugiere que en la mayor\'ia de los pa\'ises hay un peque\~no efecto positivo de la inmigraci\'on que no cambia dram\'aticamente en direcci\'on o tama\~no seg\'un mayores niveles de participaci\'on de los inmigrantes.},
  copyright = {Copyright (c) 2012},
  journal = {JSSE - Journal of Social Science Education},
  keywords = {\#duplicate-citation-key},
  language = {de}
}

