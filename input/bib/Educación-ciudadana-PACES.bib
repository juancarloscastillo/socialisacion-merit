@article{geboers_Citizenship_2015,
  title = {Citizenship Orientations and Knowledge in Primary and Secondary Education},
  author = {Geboers, Ellen and Geijsel, Femke and Admiraal, Wilfried and {ten Dam}, Geert},
  year = {2015},
  month = dec,
  volume = {18},
  pages = {749--767},
  issn = {1573-1928},
  doi = {10.1007/s11218-014-9265-7},
  abstract = {Despite widespread attention to citizenship in educational practice, knowledge of the citizenship of students is still fragmented. We therefore present a comprehensive framework to integrate empirical data and theoretical insights into the citizenship of young people today. To develop and validate the framework, we conducted exploratory and confirmative factor analyses on measures of citizenship attitudes, skills, reflection and knowledge for a sample of 7,768 students in grades 5\textendash 9 from 38 Dutch primary and secondary schools. The results were cross-validated using a different sample of 15,940 students in primary and secondary education. We were able to distinguish four citizenship orientations among students (societal interest, prosocial ability, reflective thinking and assertiveness) and two domains of citizenship knowledge (societal knowledge and interpersonal knowledge). This framework can help with the large-scale, empirical evaluation of the effects of citizenship education and also guide schools in the formulation of educational goals to support the development of citizenship competences among students.},
  journal = {Social Psychology of Education},
  language = {English},
  number = {4}
}

