@incollection{miranda_Political_2018,
  title = {The {{Political Socialization}} of {{Attitudes Toward Equal Rights}} from a {{Comparative Perspective}}},
  booktitle = {Teaching {{Tolerance}} in a {{Globalized World}}},
  author = {Miranda, Daniel and Castillo, Juan Carlos and Cumsille, Patricio},
  editor = {{Sandoval-Hern{\'a}ndez}, Andr{\'e}s and Isac, Maria Magdalena and Miranda, Daniel},
  year = {2018},
  pages = {103--123},
  publisher = {{Springer International Publishing}},
  address = {{Cham}},
  doi = {10.1007/978-3-319-78692-6_7},
  abstract = {La falta de tolerancia hacia los grupos tradicionalmente desfavorecidos, como inmigrantes, minor\'ias \'etnicas y mujeres, representa un desaf\'io creciente para democracias contempor\'aneas. Suponiendo que las actitudes hacia tales grupos sociales son, al menos en parte, aprendidos durante la socializaci\'on pol\'itica de los ni\~nos en edad escolar, este cap\'itulo explora las diferencias individuales en actitudes de igualdad de derechos utilizando datos del \'ultimo estudio internacional sobre educaci\'on c\'ivica y ciudadan\'ia (ICCS) de 2009, las caracter\'isticas socioecon\'omicas y demogr\'aficas de alumnos de octavo grado de 38 a\~nos pa\'ises. Usando ecuaciones estructurales y modelos multinivel, el an\'alisis estima modelos de regresi\'on que utilizan un conjunto de medidas, siendo el estatus familiar la principal variable independiente. Los resultados muestran que hay grandes diferencias entre pa\'ises en cuanto al nivel de actitudes inclusivas, y que la educaci\'on de los padres y el n\'umero de libros en el hogar son predictores relevantes de actitudes m\'as inclusivas de ni\~nos en la mayor\'ia de los pa\'ises analizados; sin embargo, los patrones difieren por g\'enero y grupos de inmigrantes. Los hallazgos se discuten, teniendo en cuenta los actuales y futuros problemas pol\'iticos asociados a la migraci\'on y demandas de igualdad de derechos.},
  isbn = {978-3-319-78692-6},
  keywords = {Attitudes toward diversity,International Civic and Citizenship Education Study (ICCS),International large-scale assessments,Multilevel structural equation models},
  language = {English},
  series = {{{IEA Research}} for {{Education}}}
}

