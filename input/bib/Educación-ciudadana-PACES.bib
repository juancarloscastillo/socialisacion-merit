  address = {{Singapore}},
  doi = {10.1007/978-981-10-3938-6_1},
  abstract = {This chapter traces the emergence of children and young people's participation as a field within larger processes of New Times, namely globalisation, individualisation and democratisation. The chapter argues that the success of campaigns for children and young people's participation can be attributed to this context; a context receptive to social justice, democratically-inspired language, and the political desire to recognise children and young people as politically active and capable of self-government. The chapter outlines a provocative case for the need to move beyond the `honeymoon period' of children and young people's participation.},
  isbn = {978-981-10-3938-6},
  language = {en},
  series = {Perspectives on {{Children}} and {{Young People}}}
}

