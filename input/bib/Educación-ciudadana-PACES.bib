@article{ignacz_Remains_2018,
  title = {The {{Remains}} of the {{Socialist Legacy}}: {{The Influence}} of {{Socialist Socialization}} on {{Attitudes}} toward {{Income Inequality}}},
  shorttitle = {The {{Remains}} of the {{Socialist Legacy}}},
  author = {Ign{\'a}cz, Zs{\'o}fia S.},
  year = {2018},
  month = sep,
  volume = {8},
  pages = {62},
  publisher = {{Multidisciplinary Digital Publishing Institute}},
  doi = {10.3390/soc8030062},
  abstract = {Despite convergence processes between Western and post-socialist societies in the past three decades, there are still considerable cross-country differences in individuals\&rsquo; attitudes toward income inequality. To explain these differences, studies have primarily focused on the role of macro level differences and have only theoretically acknowledged how the role of diverging socialization experiences could also be responsible. To date, little is known about the importance of socialization for attitudes toward income inequality. This article assesses whether the differences between Western and post-socialist countries are influenced by socialization effects. Applying an adapted age-period-cohort analysis on the dataset of the International Social Survey Program\&rsquo;s (ISSP) \&ldquo;Social Inequality\&rdquo; module in survey years 1992, 1999, and 2009, the paper shows that socialization has a substantial effect on attitudes and a socialist socialization clearly differentiates individuals from post-socialist countries from Westerners. Results underline that experiences gained in formative years are crucial for attitudes. A further finding is that both perception and preferences toward income inequality are influenced by socialization.},
  copyright = {http://creativecommons.org/licenses/by/3.0/},
  journal = {Societies},
  keywords = {age-period-cohort analysis,attitudes,generational differences,income inequality,post-socialist societies,socialization},
  language = {English},
  number = {3}
}

