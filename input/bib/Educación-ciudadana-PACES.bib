@article{hong_Cognitive_2021,
  title = {Cognitive {{Dissonance}} and {{Fertility Rates}}: {{A Comparative Analysis}} of {{Attitudes}} toward the {{Gender Division}} of {{Labour}} in {{East Asian}} and {{Western Industrial Societies}}},
  shorttitle = {Cognitive {{Dissonance}} and {{Fertility Rates}}},
  author = {Hong, Cheng-Huei and Gilbert, Neil},
  year = {2021},
  month = jan,
  volume = {20},
  pages = {44--61},
  issn = {1474-7464, 1475-3073},
  doi = {10.1017/S1474746420000196},
  abstract = {Based on data from the International Social Survey Program (ISSP), this study examines the extent to which cognitive dissonance regarding egalitarian attitudes toward the gender division of labour are associated with fertility rates in sixteen societies, representing Anglo American, Northern European, Western European and East Asian regimes. The findings show that although levels of support for gender equality in the realms of the family and the market are positively associated with fertility rates, an even stronger relationship to fertility emerges when weighing the differences between the levels of support for gender equality in each realm. The findings lend some corroboration to McDonald's hypothesis, which suggests that declining fertility rates are influenced by contradictory expectations between gender equality for women in the home and market.},
  journal = {Social Policy and Society},
  language = {en},
  number = {1}
}

@article{hooghe_comparative_2016,
  title = {A Comparative Analysis of `{{Good}} Citizenship': {{A}} Latent Class Analysis of Adolescents' Citizenship Norms in 38 Countries},
  shorttitle = {A Comparative Analysis of `{{Good}} Citizenship'},
  author = {Hooghe, Marc and Oser, Jennifer and Marien, Sofie},
  year = {2016},
  month = jan,
  volume = {37},
  pages = {115--129},
  issn = {0192-5121, 1460-373X},
  doi = {10.1177/0192512114541562},
  abstract = {Varios autores han afirmado que las normas de ciudadan\'ia han cambiado dr\'asticamente en las sociedades contempor\'aneas. Investigaciones recientes han estudiado las implicaciones del argumento de Russell Dalton de que las normas de ciudadan\'ia basadas en el deber (enfatizando el voto y la obediencia a la ley) est\'an siendo reemplazadas por normas de ciudadan\'ia comprometida (enfatizando formas de participaci\'on autoexpresivas y no institucionalizadas). En este art\'iculo usamos la Encuesta Internacional de Educaci\'on C\'ivica y Ciudadana de 2009 (n = 140,650) para determinar la validez emp\'irica transnacional de las normas comprometidas y basadas en el deber. Por medio del an\'alisis de clases latentes, mostramos que ambas normas de ciudadan\'ia son efectivamente adheridas por diferentes grupos de adolescentes. Sin embargo, tambi\'en mostramos que solo la mitad de la poblaci\'on de investigaci\'on mantiene estas dos normas, mientras que tambi\'en se identifican otras normas m\'as tradicionales.pol\'itico confiarEs m\'as probable que se adhieran a normas comprometidas, pero los hallazgos a nivel de pa\'is contradicen las expectativas: las normas comprometidas son menos frecuentes en las democracias estables altamente desarrolladas, y esto arroja dudas sobre la hip\'otesis de que las nuevas normas de ciudadan\'ia comprometida se encuentran predominantemente en democracias estables altamente desarrolladas. \textcopyright{} 2014, \textcopyright{} El autor (es) 2014.},
  journal = {International Political Science Review},
  keywords = {\#duplicate-citation-key},
  language = {English},
  number = {1}
}

