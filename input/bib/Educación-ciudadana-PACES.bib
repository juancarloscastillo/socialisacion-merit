@article{hooghe_Impact_2015,
  title = {The {{Impact}} of {{Education}} on the {{Development}} of {{Political Trust}}: {{Results}} from a {{Five}}-{{Year Panel Study}} among {{Late Adolescents}} and {{Young Adults}} in {{Belgium}}},
  shorttitle = {The {{Impact}} of {{Education}} on the {{Development}} of {{Political Trust}}},
