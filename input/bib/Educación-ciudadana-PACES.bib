@article{saidah_Are_2019,
  title = {Are Students Who Make an Effort Perceived as Successful or Just Liked by Their Teachers?},
  author = {Saidah, Berenice and Louvet, Eva and Pansu, Pascal},
  year = {2019},
  month = apr,
  volume = {22},
  pages = {405--419},
  publisher = {{Springer}},
  address = {{Dordrecht}},
  issn = {1381-2890},
  doi = {10.1007/s11218-019-09481-x},
  abstract = {The aim of the present paper was to analyze the social value of effort versus competence in the field of education. In Study 1, participants (N=116) were asked to indicate the importance of effort- and competence-related personality traits in achieving academic success. We also examined their meritocratic beliefs. In Study 2, middle school teachers (N=87) were asked to judge the academic success (social utility) and perceived likeability (social desirability) of students described as high versus low on effort and competence. Results showed that participants judged effort as being more important for academic success than competence, especially concerning participants showing high levels of meritocratic beliefs (Study 1). Despite this normative value of effort, effort was judged less predictive of academic success than competence, suggesting a low social value of effort with regard to social utility. A reversed pattern was observed for social desirability.},
  annotation = {WOS:000466083700008},
  journal = {Social Psychology of Education},
  keywords = {bias,causal explanations,Competence,desirability,dimensions,Effort,judgments,School judgment,self-presentation,Social norms,social value,stereotype content,utility},
  language = {English},
  number = {2}
}

